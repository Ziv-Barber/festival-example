  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "prepare": [],
    "publish": [
      "@semantic-release/github"
    ],
    "npmPublish": false,
    "verifyConditions": [
      "@semantic-release/github"
    ]
  }
}
